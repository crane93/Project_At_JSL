create table osusumeRoot(
	rnum number(5) primary key,
	rname varchar2(30),
	rpass varchar2(30),
	rtitle varchar2(50),
	rcontent varchar2(2000),
	rimgurl varchar2(200),
	rreadcount number(4) default 0,
	rwritedate date default sysdate
);

create sequence osusumeRoot_seq start with 1 increment by 1;

create table helpNotice(
	nnum number(5) primary key,
	nname varchar2(30),
	npass varchar2(30),
	ntitle varchar2(50),
	ncontent varchar2(2000),
	nwritedate date default sysdate
);

create sequence helpNotice_seq start with 1 increment by 1;
